<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="591.0" prefWidth="531.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFormController">
   <children>
      <Label alignment="CENTER" layoutX="216.0" layoutY="25.0" prefHeight="21.0" prefWidth="124.0" text="Login Form" underline="true">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="82.0" layoutY="173.0" prefHeight="21.0" prefWidth="124.0" text="User Name  :-">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="82.0" layoutY="227.0" prefHeight="21.0" prefWidth="124.0" text="Passward   :-">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPassword" layoutX="213.0" layoutY="227.0" prefHeight="25.0" prefWidth="216.0" promptText="Enter Passward" />
      <TextField fx:id="txtUserName" layoutX="213.0" layoutY="175.0" prefHeight="25.0" prefWidth="216.0" promptText="Enter User Name">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
      </TextField>
      <Button layoutX="206.0" layoutY="279.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="35.0" prefWidth="124.0" style="-fx-background-color: #2980b9;" text="Login" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="238.0" layoutY="329.0" mnemonicParsing="false" onAction="#btnRegisterOnAction" style="-fx-background-color: #f5f6fa;" text="Register" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
